{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": 0,
        "quotes": 0,
        "comma-dangle": 0,
        "max-len": 0,
        "no-restricted-syntax": 0,
        "arrow-parens": 0,
        "func-names": 0
    }
}